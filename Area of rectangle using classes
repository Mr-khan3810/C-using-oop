#include<iostream>
using namespace std;
class rectangle{
int L,B
     public:
       void get_data();
       int area();
};
class void::rectangle get_data{
      cout<<"Enter length and breadth:"<<endl;
      cin>>L,B;
}
class int::rectangle area(){
      return(L*B);
}
int main(){
      rectangle r1,r2,r3;
      r1.get_data();
      cout<<"The area of rectangle is "<<area();
      return 0;
}
